    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.18.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.1",
    "js-yaml": "^4.1.0",
